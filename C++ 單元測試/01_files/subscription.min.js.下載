(function($){pix.getCloudFeedSubscriptions=function(uid,limit,layout){if(!limit){limit=5}$.getJSON("//cloudfeed.pixplug.in/subscription?uid="+uid+"&limit="+limit+"&callback=?",function(data){if(!layout){layout="pixnet"}if(data.error){return false}if(data.total<1){return false}var i=0;switch(layout){case"grey":case"grey2":$.each(data.subscriptions,function(key,val){if(val.lastest_time>0){var sub_title='<span class="SubEntryTitle">'+pix.htmlspecialchars(val.title)+"</span>";var dt=new Date(val.lastest_time*1e3);var sub_date=dt.getFullYear()+"-"+(dt.getMonth()+1)+"-"+dt.getDate();var sub_link='<span class="SubEntryBody"><a href="'+pix.htmlspecialchars(val.lastest_url)+'" target="_blank">'+pix.htmlspecialchars(val.lastest_topic)+"</a></span><br/>";$("#subscription .Sidebody").append(sub_title+"("+sub_date+")"+":"+sub_link)}});break;case"wretch":$.each(data.subscriptions,function(key,val){if(val.lastest_time>0){var sub_title='<span id="boxSubEntryTitle">'+pix.htmlspecialchars(val.title)+"</span>";var dt=new Date(val.lastest_time*1e3);var sub_date=dt.getFullYear()+"-"+(dt.getMonth()+1)+"-"+dt.getDate();var sub_link='<span id="boxSubEntryBody"><a href="'+pix.htmlspecialchars(val.lastest_url)+'" target="_blank">'+pix.htmlspecialchars(val.lastest_topic)+"</a></span><br/>";$("#boxSubList .side").append(sub_title+"("+sub_date+")"+"<br/>"+sub_link)}});break;case"yam":$.each(data.subscriptions,function(key,val){if(val.lastest_time>0){var sub_title='<span class="SubEntryTitle">'+pix.htmlspecialchars(val.title)+"</span>";var dt=new Date(val.lastest_time*1e3);var sub_date=dt.getFullYear()+"-"+(dt.getMonth()+1)+"-"+dt.getDate();var sub_link='<span class="boxSubEntryBody"><a href="'+pix.htmlspecialchars(val.lastest_url)+'" target="_blank">'+pix.htmlspecialchars(val.lastest_topic)+"</a></span>";$("#boxSubList").append('<div class="sidebody">'+sub_title+"("+sub_date+")"+":"+sub_link+"</div>")}});break;case"pixnet":default:var items=[];$.each(data.subscriptions,function(key,val){if(val.lastest_time>0){var sub_link='<a href="'+pix.htmlspecialchars(val.lastest_url)+'" target="_blank">'+pix.htmlspecialchars(val.lastest_topic)+"</a>";items.push("<li>"+sub_link+"<span> from "+pix.htmlspecialchars(val.title)+"</span>"+"</li>")}});$("<ul/>",{html:items.join("")}).appendTo("#subscription div");break}})}})(jQuery);
